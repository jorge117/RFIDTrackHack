CREATE TABLE PERSON
(
  RFID INT NOT NULL,
  Fname INT NOT NULL,
  Sname INT NOT NULL,
  DOB INT NOT NULL,
  Gender INT NOT NULL,
  CampNo INT NOT NULL,
  BedNo INT NOT NULL,
  TentNo INT NOT NULL,
  FamilyID INT NOT NULL,
  PRIMARY KEY (RFID),
  FOREIGN KEY (FamilyID) REFERENCES FAMILY(FamilyID)
);

CREATE TABLE FAMILY
(
  FamilyID INT NOT NULL,
  RFID INT NOT NULL,
  PRIMARY KEY (FamilyID),
  FOREIGN KEY (RFID) REFERENCES PERSON(RFID)
);